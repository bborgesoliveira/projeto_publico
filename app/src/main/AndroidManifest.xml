<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bruno.convenios">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Convenios"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FiltroActivity"
            android:label="@string/title_activity_filtro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResumoActivity"
            android:label="@string/title_activity_resumo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TabelaActivity"
            android:label="@string/title_activity_tabela"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
